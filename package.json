{
  "name": "hybrid-protocol",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run compile && npx hardhat typechain",
    "clean": "npx hardhat clean",
    "compile": "npx hardhat compile",
    "test": "npx hardhat test",
    "coverage": "npm run build && npx hardhat coverage --temp artifacts --network coverage",
    "rebuild": "npm run clean && npm run build && npm run test",
    "lint": "eslint ./ --fix",
    "lint-quick": "pretty-quick --staged --pattern \"**/*.*(ts|tsx)\"",
    "deploy": "npm run rebuild && npx hardhat run scripts/deploy.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-quick",
      "pre-push": "npm run lint"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HybridProtocol/hybrid-protocol"
  },
  "author": "nbailo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/HybridProtocol/hybrid-protocol/issues"
  },
  "homepage": "https://github.com/HybridProtocol/hybrid-protocol#readme",
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^2.0.0",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
    "@openzeppelin/contracts": "^3.1.0",
    "@openzeppelin/upgrades": "^2.8.0",
    "@typechain/ethers-v4": "^1.0.0",
    "@typechain/truffle-v4": "^2.0.2",
    "@typechain/truffle-v5": "^2.0.2",
    "@typechain/web3-v1": "^1.0.0",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^7.0.2",
    "@types/node": "^14.14.6",
    "@typescript-eslint/eslint-plugin": "^3.1.0",
    "@typescript-eslint/parser": "^3.1.0",
    "chai": "^4.2.0",
    "dotenv": "^8.2.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "ethereum-waffle": "^3.2.0",
    "ethers": "^5.0.19",
    "hardhat": "^2.0.2",
    "hardhat-typechain": "^0.3.3",
    "husky": "^4.2.5",
    "prettier": "^2.0.5",
    "pretty-quick": "^2.0.1",
    "solidity-coverage": "^0.7.5",
    "ts-generator": "0.0.8",
    "ts-node": "^8.10.2",
    "typechain": "^2.0.1",
    "typechain-target-ethers-v5": "^1.2.2",
    "typescript": "^3.9.7"
  }
